{
  "nóri-grammar": {
    "templates": {
      "[adjective]": {
        "hint": [ "[adjective]" ],
        "match": [ "[adjective]" ]
      },
      "[adjective] [noun]": {
        "hint": [ "[adjective] [noun]" ],
        "match": [ "a [adjective] [noun]", "[adjective] [noun]" ]
      },
      "[demonstrative]": {
        "hint": ["[demonstrative]"],
        "match": ["[demonstrative]"]
      },
      "i [adjective]": {
        "hint": ["the [adjective] (one/thing)"],
        "match": ["the [adjective] one", "the [adjective] thing", "the [adjective] ones", "the [adjective] things"]
      },
      "i [noun]": {
        "hint": ["the [noun]"],
        "match": ["the [noun]"]
      },
      "[noun]": {
        "hint": [ "(a) [noun]" ],
        "match": [ "[noun]", "a [noun]" ]
      },
      "[preverb] lá": {
        "hint": [ "(he/she/it/they) [preverb]" ],
        "match": [
          "[preverb]",
          "[preverb]s",
          "he [preverb]s",
          "she [preverb]s",
          "it [preverb]s",
          "they [preverb]"
        ]
      },
      "[pronoun]": {
        "hint": [ "[pronoun]" ],
        "match": [ "[pronoun]" ]
      },
      "ny lá": {
        "hint": [ "I am" ],
        "match": [ "I'm", "I am" ]
      },
      "ké lá": {
        "hint": [ "you are" ],
        "match": [ "you're", "you are" ]
      },
      "ćy lá": {
        "hint": [ "we are" ],
        "match": [ "we're", "we are" ]
      },
      "vun lá": {
        "hint": [ "you are" ],
        "match": [ "you're", "you are" ]
      }
    }
  },
  "nóri": {
