{
  "nórem-grammar": {
    "templates": {
      "i [adjective]": {
        "hint": ["the [adjective] one/thing"],
        "match": ["the [adjective] one", "the [adjective] thing", "the [adjective] ones", "the [adjective] things"]
      },
      "i [noun]": {
        "hint": ["the [noun]"],
        "match": ["the [noun]"]
      },
      "[preverb] han": {
        "hint": ["[preverb]", "(he/she/it) [preverb]", "(they) [preverb]"],
        "match": ["[preverb]", "he [preverb]s", "she [preverb]s", "it [preverb]s", "they [preverb]"]
      },
      "[preverb] hotá": {
        "hint": ["do not [preverb]", "(he/she/it) does not [preverb]", "(they) do not [preverb]"],
        "match": ["do not [preverb]", "he does not [preverb]", "she does not [preverb]", "it does not [preverb]", "they do not [preverb]"]
      }
    }
  },
  "nórem": {
    "amif": {
      "ipa": "ˈaˌmif",
      "pos": "adjective",
      "hint": ["bad"],
      "match": ["bad"],
      "definition": "Bad, especially in the views of the speaker"
    },
    "sa": {
      "ipa": "ˈsa",
      "pos": "adjective",
      "hint": ["good"],
      "match": ["good", "well"],
      "definition": "Good, well, or favorable, especially in the views of the speaker"
    },

    "mikol": {
      "ipa": "ˈmiˌkol",
      "pos": "adjective",
      "simple": ["short", "small"],
      "hint": ["short", "small"],
      "match": ["short", "small"],
      "definition": "Short or small, especially in comparison to the subject"
    },
    "náu": {
      "ipa": "ˈnau˦˧",
      "pos": "adjective",
      "hint": ["tall", "large", "big"],
      "match": ["tall", "large", "big"],
      "definition": "Tall or big, especially in comparison to the subject."
    },
    "sorón": {
      "ipa": "ˈsoˌron˦",
      "pos": "adjective",
      "definition": "Kind or nice",
      "hint": ["kind", "nice"],
      "match": ["kind", "nice"]
    },
    
    "i": {
      "ipa": "ˈi",
      "pos": "article",
      "definition": "denoting one or more people or things already mentioned or assumed to be common knowledge.",
      "simple": ["the"],
      "hint": ["the"],
      "match": ["the"]
    },
    
    "ele": {
      "ipa": "ˈeˌle",
      "pos": "conjunction",
      "definition": "Used to connect words of the same part of speech, clauses, or sentences meant to be taken jointly; and",
      "hint": ["and"],
      "match": ["and"]
    },
    
    "cévo": {
      "ipa": "ˈt͡se˦ˌʋo",
      "pos": "noun",
      "definition": "A female human attaining the age of adulthood; woman. When referring to children, it signifies their physical and/or emotional maturity.",
      "hint": ["(a) woman"],
      "match": ["woman", "a woman"]
    },
    "ćina": {
      "ipa": "ˈt͡ʃiˌna",
      "pos": "noun",
      "definition": "Any given person.",
      "hint": ["(a) person"],
      "match": ["person", "a person"]
    },
    "lavat": {
      "ipa": "ˈlaˌʋat",
      "pos": "noun",
      "hint": ["(an) apple"],
      "match": ["apple", "an apple"],
      "definition": "Any given apple."
    },
    "miro": {
      "ipa": "ˈmiˌro",
      "pos": "noun",
      "hint": ["(a loaf of) bread"],
      "match": ["bread", "bread loaf", "a bread loaf", "loaf of bread", "a loaf of bread"],
      "definition": "Any given loaf of bread. When necessary, leavened bread."
    },
    "mou": {
      "ipa": "ˈmou",
      "pos": "noun",
      "hint": ["water"],
      "match": ["water"],
      "definition": "water."
    },
    "nes": {
      "ipa": "ˈnes",
      "pos": "noun",
      "hint": ["(a) thing", "(an) object"],
      "match": ["thing", "a thing"],
      "definition": "Any given thing or object."
    },
    "symi": {
      "ipa": "ˈsəˌmi",
      "pos": "noun",
      "definition": "A male human attaining the age of adulthood; man. When referring to children, it signifies their physical and/or emotional maturity.",
      "simple": ["man"],
      "hint": ["(a) man"],
      "match": ["man", "a man"]
    },

    "mai": {
      "ipa": "ˈmai",
      "pos": "noun",
      "hint": ["(a loaf of) unleavened bread", "(a) cracker"],
      "match": ["unleavened bread", "loaf of unleavened bread", "a loaf of unleavened bread", "cracker", "a cracker"],
      "definition": "Refering to unleavened bread"
    },
    
    "kas": {
      "ipa": "ˈkas",
      "pos": "preverb",
      "simple": ["eat", "drink"],
      "hint": ["eat", "drink"],
      "match": ["eat", "eats", "drink", "drinks"],
      "definition": "Used to describe the possession of nouns."
    },
    "vé": {
      "ipa": "ˈʋe˦",
      "pos": "preverb",
      "simple": ["have"],
      "hint": ["have"],
      "match": ["have", "has"],
      "definition": "Used to describe the possession of nouns."
    },
    "vé han": {
      "hint": ["have", "(he/she/it) has"],
      "match": ["have", "has", "he has", "she has", "it has"]
    }, 
    
    "ćy": {
      "ipa": "ˈt͡ʃə",
      "pos": "pronoun",
      "hint": ["we", "us"],
      "match": ["we", "us"],
      "definition": "Used by a speaker to refer to themself and another person as the subject or object of a sentence; we"
    },
    "ké": {
      "ipa": "ˈke˦",
      "pos": "pronoun",
      "hint": ["you (NOM.SING.)"],
      "match": ["you"],
      "definition": "Used to informally refer to a person the speaker is addressing in the subject; you"
    },
    "ny": {
      "ipa": "ˈnə",
      "pos": "pronoun",
      "hint": ["I", "me"],
      "match": ["i", "me"],
      "definition": "Used by a speaker to refer to themself as the subject or object of a sentence; I"
    },
    "téu": {
      "ipa": "ˈteu˦˧",
      "pos": "pronoun",
      "hint": ["him", "her", "it", "them"],
      "match": ["him", "her", "it", "them"],
      "definition": "Used to refer a person or group other than the speaker or addressee as the object of a sentence; him/her/it/them"
    },
    "vu": {
      "ipa": "ˈʋu",
      "pos": "pronoun",
      "hint": ["you (ACC.SING.)"],
      "match": ["you"],
      "definition": "Used to informally refer to a person the speaker is addressing in the object; you"
    },
    "vun": {
      "ipa": "ˈʋun",
      "pos": "pronoun",
      "hint": ["you (PLUR.)"],
      "match": ["you", "you guys", "you all"],
      "definition": "Used to informally refer to the people the speaker is addressing; y'all"
    },
    
    "han": {
      "ipa": "ˈxan",
      "pos": "verb",
      "definition": "Correlating the relation between nouns or the existence of a noun.",
      "simple": ["be", "exist"],
      "hint": ["be", "exist", "(he/she/it) is", "(he/she/it) exists"],
      "match": ["be", "am", "are", "is", "he is", "she is", "it is", "they are", "exist", "exists", "he exists", "she exists", "it exists", "they exist"]
    },
    "hotá": {
      "ipa": "ˈxoˌta˦",
      "pos": "verb",
      "definition": "Correlating the irrelation between nouns or the existence of a noun.",
      "hint": ["not be", "not", "(he/she/it) is not"],
      "match": ["not be", "not", "am not", "are not", "is not", "he is not", "she is not", "it is not", "they are not", "do not exist", "he does not exist", "she does not exist", "it does not exist", "they do not exist"]
    }
  }
}