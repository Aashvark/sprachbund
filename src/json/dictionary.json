{
  "nóri-grammar": {
    "templates": {
      "[adjective]": {
        "hint": ["[adjective]"],
        "match": ["[adjective]"]
      },
      "[adjective] [noun]": {
        "hint": ["[adjective] [noun]"],
        "match": ["a [adjective] [noun]"]
      },
      "[demonstrative]": {
        "hint": ["[demonstrative]"],
        "match": ["[demonstrative]"]
      },
      "[demonstrative] [adjective]": {
        "hint": ["[demonstrative] [adjective] one/thing"],
        "match": ["[demonstrative] [adjective] one", "[demonstrative] [adjective] thing", "[demonstrative] [adjective] ones", "[demonstrative] [adjective] things"]
      },
      "[demonstrative] [adjective] [noun]": {
        "hint": ["[demonstrative] [adjective] [noun]"],
        "match": ["[demonstrative] [adjective] [noun]"]
      },
      "[demonstrative] [noun]": {
        "hint": ["[demonstrative] [noun]"],
        "match": ["[demonstrative] [noun]"]
      },
      "i [adjective]": {
        "hint": ["the [adjective] one/thing"],
        "match": ["the [adjective] one", "the [adjective] thing", "the [adjective] ones", "the [adjective] things"]
      },
      "i [adjective] [noun]": {
        "hint": ["the [adjective] [noun]"],
        "match": ["the [adjective] [noun]"]
      },
      "i [noun]": {
        "hint": ["the [noun]"],
        "match": ["the [noun]"]
      },
      "[interjection]": {
        "hint": ["[interjection]"],
        "match": ["[interjection]"]
      },
      "[preverb] han": {
        "hint": ["[preverb]", "(he/she/it) [preverb]s", "(they) [preverb]"],
        "match": ["[preverb]", "[preverb]s", "he [preverb]s", "she [preverb]s", "it [preverb]s", "they [preverb]"]
      },
      "[preverb] hotá": {
        "hint": ["do not [preverb]", "(he/she/it) does not [preverb]", "(they) do not [preverb]"],
        "match": ["don't [preverb]", "doesn't [preverb]", "he doesn't [preverb]", "she doesn't [preverb]", "it doesn't [preverb]", "they don't [preverb]", "do not [preverb]", "does not [preverb]", "he does not [preverb]", "she does not [preverb]", "it does not [preverb]", "they do not [preverb]"]
      },
      "ny vé han ćimon": {
        "hint": ["I am sorry"],
        "match": ["I'm sorry", "I am sorry"]
      },
      "ny han": {
        "hint": ["I am"],
        "match": ["I'm", "I am"]
      },
      "ny hotá": {
        "hint": ["I am not"],
        "match": ["I'm not", "I am not"]
      },
      "ké han": {
        "hint": ["you are"],
        "match": ["you're", "you are"]
      },
      "ké hotá": {
        "hint": ["you are not"],
        "match": ["you're not", "you aren't", "you are not"]
      },
      "ćy han": {
        "hint": ["we are"],
        "match": ["we're", "we are"]
      },
      "ćy hotá": {
        "hint": ["we are not"],
        "match": ["we're not", "we aren't", "we are not"]
      },
      "vun han": {
        "hint": ["you are"],
        "match": ["you're", "you are"]
      },
      "vun hotá": {
        "hint": ["you are not"],
        "match": ["you're not", "you aren't", "you are not"]
      }
    }
  },
  "nóri": {
    "eu": {
